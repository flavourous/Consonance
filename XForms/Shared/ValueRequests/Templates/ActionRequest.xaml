<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
	xmlns:l="clr-namespace:Consonance.XamarinFormsView;assembly=Consonance.XamarinFormsView"
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	x:Class="Consonance.XamarinFormsView.ActionRequest">
	<Grid HorizontalOptions="End">
		<Grid.RowDefinitions>
			<RowDefinition Height="30"/>
		</Grid.RowDefinitions>
		<Label Text="{Binding name}" VerticalOptions="Center" HorizontalOptions="Center"/>
		<Button Clicked="OnClicked"/>
	</Grid>
</ContentView>
